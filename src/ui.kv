 #:kivy 1.11.1

WindowManager:
    MainWindow:
    SecondWindow:

<MainWindow>:
    name:"Main"
	id: main
    
    GridLayout:
        cols:1

    	Image:
            id: img

		GridLayout:
			cols:3		

		    Button:
		        text:"Register"
				size_hint_y: None	
				height: '48dp'
		        on_release: 
		            self.parent.parent.parent.printPixels(camera)

			Button:
		        text:"Guest"
				size_hint_y: None
				height: '48dp'
		        on_release: 
		            app.root.current = "Second"

			Button:
		        text:"Problem ?"
				size_hint_y: None
				height: '48dp'
		        on_release: 
		            app.root.current = "Second"

<SecondWindow>:
    name:"Second"

    Button:
        text:"Go Back"
        on_release: 
            app.root.current = "Main"
